    {{#with data}}
  <h1>{{title}}</h1>

  {{#each introContent}}
    {{{this}}}
  {{/each}}

  {{#each contents}}
    <h2>{{subTitles}}</h2>

    <iframe
      width="560"
      height="315"
      src="https://www.youtube.com/embed/{{idVideo}}"
      frameborder="0"
      allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture"
      allowfullscreen
    ></iframe>

    {{#each array}}
      {{{this}}}
    {{/each}}
  {{/each}}
{{/with}}